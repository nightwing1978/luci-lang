for (x in [1,2,3])
{
    print(x);
};

for (const x : int in {4,5,6})
{
    print(x);
};

for (x in {"a" : 1, "b": 2, "c" : 3})
{
    print(x);
};

for (x in "xyz")
{
    print(x);
}

let a = [1,2,3];
for (x in a)
{
    print(x);
    a.clear();
};

let b = {4,5,6};
for (const x : int in b)
{
    print(x);
    b.clear();
};
